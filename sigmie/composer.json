{
    "name": "sigmie/sigmie",
    "description": "Sigmie Elasticsearch library",
    "keywords": [
        "elasticsearch",
        "sigmie"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "nicoorfi",
            "email": "nico@sigmie.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sigmie\\": "src/"
        },
        "exclude-from-classmap": [
            "/test/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sigmie\\": "src/",
            "Sigmie\\Tests\\": "tests/"
        }
    },
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/var-dumper": "^4.3"
    },
    "archive": {
        "exclude": [
            "!vendor",
            "tests",
            "*phpunit.xml",
            "composer.phar",
            "lib/vendor",
            "*.swp"
        ]
    }
}